<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="softwaretestingmaterial">
	<groups>
		<run>
			<include name="funcTest"/>
		</run>
	</groups>
	<listeners>
		<listener class-name="Common.Listener.Listener"></listener>
	</listeners>
	<test name="functionalTest">
		<classes>
			<class name="Tests.CalendarTest" />
			<class name="Tests.SearchTest" />
		</classes>
	</test>
</suite>
